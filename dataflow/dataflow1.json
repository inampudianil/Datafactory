{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ADLSGen2",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          userTime as date,",
				"          msgTime as string,",
				"          seqNo as string,",
				"          programName as string,",
				"          machineID as string,",
				"          moduleName as string,",
				"          functionName as string,",
				"          line as string,",
				"          severity as string,",
				"          errVal as string,",
				"          msg as string,",
				"          {rowStatus\\r} as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     partitionBy('hash', 1)) ~> source1",
				"source1 derive(msg = fromBase64('Z3VuY2h1cw==')) ~> derivedColumn1"
			]
		}
	}
}
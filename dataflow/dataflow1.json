{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ADLSGen2",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          userTime as date,",
				"          msgTime as string,",
				"          seqNo as string,",
				"          programName as string,",
				"          machineID as string,",
				"          moduleName as string,",
				"          functionName as string,",
				"          line as string,",
				"          severity as string,",
				"          errVal as string,",
				"          msg as string,",
				"          {rowStatus\\r} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false) ~> source1"
			]
		}
	}
}